<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity2">

    <Button
        android:id="@+id/is_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="蓝牙是否打开" />


    <Button
        android:id="@+id/open_blue"
        app:layout_constraintTop_toBottomOf="@+id/is_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开蓝牙" />

    <Button
        android:id="@+id/apply_work"
        android:text="apply work"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/print_work"
        android:text="print work"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
<Button
        android:id="@+id/init_work"
        android:text="init work"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>



</androidx.appcompat.widget.LinearLayoutCompat>